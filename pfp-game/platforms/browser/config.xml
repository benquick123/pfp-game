<?xml version='1.0' encoding='utf-8'?>
<widget id="pfp.vasko" version="0.9.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>V.A.S.K.O.</name>
    <description>
        A persons from Porlock game.
    </description>
    <author email="personsfromporlockpfp@gmail.com" href="http://cordova.io">
        Persons from Porlock
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="landscape" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
</widget>
